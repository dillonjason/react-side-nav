{
  "name": "react-side-nav",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "private": true,
  "description": "node microapp for react-side-nav",
  "scripts": {
    "start": "node util/es6_mode --file src/server/server.js",
    "predev": "npm run clean",
    "dev": "NODE_ENV=dev nodemon --inspect util/es6_mode --file src/server/server.js",
    "clean": "rm -rf dist && npm prune",
    "build": "webpack --config util/webpack/prod.js ",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "XUNIT_FILE=./test_results/unit.xml mocha --opts test/unit/mocha.opts",
    "test:unit:dev": "mocha --opts test/unit/mocha_dev.opts",
    "pretest:integration": "babel test/integration/index._test.js --out-file integration_tests.js",
    "test:integration": "casperjs test integration_tests.js --xunit=test_results/integration.xml",
    "lint": "eslint --cache true --ext .jsx,.js ./src/**",
    "lint:fix": "eslint --ext .jsx,.js --fix ./src/**"
  },
  "dependencies": {
    "app-module-path": "2.2.0",
    "autoprefixer-loader": "3.2.0",
    "babel-cli": "6.24.1",
    "babel-loader": "7.1.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.24.1",
    "css-loader": "0.28.4",
    "enzyme": "2.9.1",
    "eslint": "4.2.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-react": "5.0.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.1.0",
    "eslint-plugin-standard": "3.0.1",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "handlebars": "4.0.10",
    "html-webpack-harddisk-plugin": "0.1.0",
    "html-webpack-plugin": "2.29.0",
    "json-loader": "0.5.4",
    "koa": "2.3.0",
    "koa-bodyparser": "4.2.0",
    "koa-ejs": "4.1.0",
    "koa-json": "2.0.2",
    "koa-mount": "3.0.0",
    "koa-render": "0.2.1",
    "koa-router": "7.2.1",
    "koa-static": "4.0.1",
    "koa-webpack-dev-middleware": "1.4.6",
    "koa-webpack-hot-middleware": "1.0.3",
    "lodash": "4.17.4",
    "minimist": "1.2.0",
    "node-sass": "4.5.3",
    "nodemon": "1.11.0",
    "react": "15.6.1",
    "react-addons-test-utils": "15.6.0",
    "react-dom": "15.6.1",
    "react-transform-hmr": "1.0.4",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "webpack": "3.2.0"
  },
  "devDependencies": {
    "babel-preset-env": "1.6.0",
    "prop-types": "15.5.10",
    "webpack-merge": "4.1.0"
  }
}
